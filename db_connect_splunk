SELECT [UDLP_Incidents].[IncidentId]
      ,[UDLP_Incidents].[ViolationLocalTime]
      ,[UDLP_Incidents].[ComputerID]
      ,[UDLP_Incidents].[UserID]
      ,[UDLP_Incidents].[Severity]
      ,[UDLP_Incidents].[StatusId]
      ,[UDLP_Incidents].[ResolutionId]
      ,[UDLP_Incidents].[SourceApplicationId]
      ,[UDLP_Incidents].[SourceApplicationTemplates]
      ,[UDLP_Incidents].[MatchUrl]
      ,[UDLP_Incidents].[ClassificationsToDisplay]
      ,[UDLP_Incidents].[RulesToDisplay]
      ,[UDLP_Incidents].[destination]
      ,[UDLP_Incidents].[ShortMatchString]
      ,[UDLP_Incidents].[RuleSetToDisplay]
      ,[UDLP_Incidents].[UserPrincipalName]
      ,[UDLP_Incidents].[UserAccount]
      ,[UDLP_Incidents].[UserEmail]
      ,[UDLP_Incidents].[UserOU]
	  ,[UDLP_EventUsers].UserName
	  ,[UDLP_EventComputers].[Name]
	  ,[UDLP_IncidentStatuses].[StatusKey]
	  ,[UDLP_IncidentResolutions].[ResolutionKey]
  FROM [ePO_EPO].[dbo].[UDLP_Incidents]
INNER JOIN [UDLP_EventUsers] on [UDLP_EventUsers].[UserId] =  [UDLP_Incidents].[UserID]
INNER JOIN [UDLP_EventComputers] on [UDLP_EventComputers].ID = [UDLP_Incidents].[ComputerID]
INNER JOIN [UDLP_IncidentStatuses] on [UDLP_IncidentStatuses].StatusID = [UDLP_Incidents].StatusId 
INNER JOIN [UDLP_IncidentResolutions] on [UDLP_IncidentResolutions].ResolutionID = [UDLP_Incidents].ResolutionId
WHERE [UDLP_Incidents].[IncidentId] > ? 
ORDER BY [UDLP_Incidents].[IncidentId] ASC
